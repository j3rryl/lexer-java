<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PrimaryController">
   <Button fx:id="tokenizeButton" text="Tokenize" />
   <HBox spacing="12.0">
      <TextArea fx:id="codeInput" promptText="Enter code here" />
      <TextArea fx:id="resultOutput" editable="false" />
      <padding>
         <Insets top="2.0" />
      </padding>
      <VBox.margin>
         <Insets top="12.0" />
      </VBox.margin>
   </HBox>
   <padding>
      <Insets top="2.0" />
   </padding>
</VBox>
